{
  "name": "map-app",
  "version": "1.0.0",
  "private": true,
  "description": "map app react",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,ts,jsx,tsx}": [
      "npx prettier --write --single-quote",
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@types/jest": "^29.2.2",
    "@types/lodash": "^4.14.189",
    "@types/mapbox-gl": "^2.7.7",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "classnames": "^2.3.2",
    "i18next": "^22.0.5",
    "lodash": "^4.17.21",
    "mapbox-gl": "^2.9.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-i18next": "^12.0.0",
    "react-redux": "^8.0.5",
    "react-scripts": "^4.0.3",
